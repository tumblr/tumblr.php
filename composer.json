{
  "name": "tumblr/tumblr",
  "type": "library",
  "description": "Official Tumblr PHP Client",
  "keywords": [
    "tumblr",
    "api",
    "sdk",
    "gif"
  ],
  "homepage": "https://github.com/tumblr/tumblr.php",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "John Crepezzi",
      "email": "john.crepezzi@gmail.com",
      "homepage": "https://github.com/seejohnrun",
      "role": "developer"
    }
  ],
  "require": {
    "eher/oauth": "^1.0.7 <1.1",
    "guzzlehttp/guzzle": "^6.5.2 || ^7.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.7.27 <10"
  },
  "autoload": {
    "psr-0": {
      "Tumblr\\API": "lib"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "": "tests"
    }
  },
  "config": {
    "allow-plugins": true
  }
}
